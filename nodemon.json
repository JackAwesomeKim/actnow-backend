{
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "dist"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "events": {
      "restart": "webpack --config ./config/webpack.common.js"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
}